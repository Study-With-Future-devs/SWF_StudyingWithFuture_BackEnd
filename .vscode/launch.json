{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Rodar Script SQL",
            "type": "PowerShell",
            "request": "launch",
            "script": "./Studying-With-Future/Run-SQL.ps1",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false
        },
        {
            "name": "Dotnet Run",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet-build",
            "program": "dotnet",
            "args": [
                "run",
                "--project",
                "${workspaceFolder}/Studying-With-Future/Studying-With-Future.csproj"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
         }
    ]
}
